class Solution:
    def maxSubarraySum(self, arr, k):
        n = len(arr)
        maximum_sum = 0
        subarray_sum = 0
        left = 0
        right = 0
        
        while right < n :
            subarray_sum += arr[right]
            if (right - left) >= k :
                subarray_sum -= arr[left]
                left += 1
            maximum_sum = max(maximum_sum, subarray_sum)
            right += 1
        
        return maximum_sum
